{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "~8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "api-platform/core": "^2.6.6",
        "doctrine/doctrine-bundle": "^2.5.0",
        "doctrine/orm": "^2.10.2",
        "nelmio/cors-bundle": "^2.1.1",
        "prooph/common": "^4.5.0",
        "prooph/event-store": "^7.6.1",
        "prooph/event-store-symfony-bundle": "^0.9.3",
        "prooph/pdo-event-store": "^1.13.0",
        "symfony/asset": "^5.3.11",
        "symfony/console": "^5.3.11",
        "symfony/dependency-injection": "^5.3.11",
        "symfony/dotenv": "^5.3.10",
        "symfony/flex": "^1.17.5",
        "symfony/framework-bundle": "^5.3.11",
        "symfony/http-kernel": "^5.3.12",
        "symfony/mercure-bundle": "^0.3.3",
        "symfony/messenger": "^5.3.11",
        "symfony/routing": "^5.3.11",
        "symfony/runtime": "^5.3.11",
        "symfony/twig-bundle": "^5.3.10",
        "symfony/uid": "^5.3.10",
        "symfony/validator": "^5.3.12",
        "symfony/yaml": "^5.3.11",
        "twig/twig": "^3.3.4"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0.0",
        "ergebnis/composer-normalize": "^2.16.0",
        "maglnet/composer-require-checker": "^3.5.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpstan/phpstan-strict-rules": "^1.1.0",
        "phpstan/phpstan-symfony": "^1.0.1",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^3.0.4",
        "symfony/debug-bundle": "^5.3.4",
        "symfony/phpunit-bridge": "^5.3.11",
        "symfony/var-dumper": "^5.3.11",
        "symfony/web-profiler-bundle": "^5.3.8",
        "symplify/config-transformer": "^9.4.70",
        "symplify/easy-coding-standard": "^9.4.70",
        "vimeo/psalm": "^4.13.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "autoload": {
        "files": [
            "src/Core/Infrastructure/Php/Spl/functions.php"
        ],
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
