{
    "name": "lctrs/apiplatform-ddd-cqrs-es-demo",
    "description": "",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "dev-master",
        "doctrine/annotations": "^1.0",
        "doctrine/dbal": "^2.8",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.4.5",
        "guzzlehttp/guzzle": "^6.3",
        "marc-mabe/php-enum": "^3.0",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^4.3",
        "prooph/common": "^4.2",
        "prooph/event-store": "^7.3",
        "prooph/event-store-symfony-bundle": "^0.5.0",
        "prooph/pdo-event-store": "^1.9",
        "ramsey/uuid": "^3.8",
        "symfony/asset": "*",
        "symfony/config": "*",
        "symfony/console": "*",
        "symfony/dependency-injection": "*",
        "symfony/doctrine-bridge": "*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "*",
        "symfony/http-kernel": "*",
        "symfony/messenger": "*",
        "symfony/routing": "*",
        "symfony/serializer": "*",
        "symfony/twig-bundle": "*",
        "symfony/yaml": "*"
    },
    "require-dev": {
        "doctrine/coding-standard": "^5.0",
        "nelmio/alice": "^3.4",
        "phpstan/phpstan": "^0.10.2",
        "phpstan/phpstan-doctrine": "^0.10.0",
        "phpstan/phpstan-symfony": "^0.10.0",
        "symfony/finder": "*",
        "symfony/profiler-pack": "^1.0",
        "theofidry/alice-data-fixtures": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "devtools/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check": [
            "@cs-check",
            "@analyze"
        ],
        "analyze": "phpstan analyze -l max -c ./phpstan.neon ./src ./config",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.2.*"
        }
    }
}
